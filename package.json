    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.17.0",
    "express": "^4.19.2",
    "happy-dom": "^14.12.3",
    "json-server": "^0.17.4",
    "msw": "^1.3.3",
    "node-fetch": "^3.3.2",
