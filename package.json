{
  "name": "code",
  "type": "commonjs",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@7.28.0",
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "scripts": {
    "dev": "run-p dev:vite dev:test-server dev:api",
    "dev:vite": "vite",
    "dev:test-server": "vitest -c tests/vitest.config.ts --ui",
    "dev:api": "json-server -p 7777 -w db.json",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint --ext=.ts,.vue src tests",
    "dev:test": "vitest",
    "internal:test": "IS_TEST_RUN=yes vitest -c tests/vitest.config.ts --no-api",
    "internal:test:types": "vue-tsc --noEmit --skipLibCheck"
  },
