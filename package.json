{
  "name": "code",
  "type": "commonjs",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@8.6.4",
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "scripts": {
    "dev": "run-p dev:vite dev:test-server dev:api",
    "dev:vite": "node server.mjs --open",
    "dev:test-server": "vitest -c tests/vitest.config.ts --ui",
    "dev:api": "json-server -p 7777 -w db.json",
    "build": "pnpm run build:client && pnpm run build:server",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.ts --outDir dist/server",
    "serve": "NODE_ENV=production node server.mjs",
    "lint": "eslint --ext=.ts,.vue src tests",
    "dev:test": "vitest",
    "internal:test": "IS_TEST_RUN=yes vitest run -c tests/vitest.config.ts --no-api",
    "internal:test:types": "vue-tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "@exampledev/new.css": "^1.1.3",
    "@vueuse/core": "^10.4.1",
    "animate.css": "^4.1.1",
    "lodash-es": "^4.17.21",
    "mande": "^2.0.7",
    "pinia": "^2.1.6",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@types/compression": "^1.7.3",
    "@types/express": "^4.17.17",
    "@types/lodash-es": "^4.17.9",
    "@types/node-fetch": "^2.6.5",
    "@types/serve-static": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vitest/ui": "^0.34.3",
    "@vitest/ws-client": "^0.34.3",
    "@vue/compiler-sfc": "^3.3.4",
    "@vue/test-utils": "^2.4.1",
    "autoprefixer": "^10.4.15",
    "compression": "^1.7.4",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.17.0",
    "express": "^4.18.2",
    "happy-dom": "^10.11.2",
    "json-server": "^0.17.3",
    "msw": "^1.3.1",
    "node-fetch": "^3.3.2",
    "npm-run-all": "^4.1.5",
    "open": "^9.1.0",
    "postcss": "^8.4.30",
    "serve-static": "^1.15.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "unplugin-vue-components": "^0.25.2",
    "unplugin-vue-markdown": "^0.24.3",
    "unplugin-vue-router": "^0.6.4",
    "vite": "^4.4.9",
    "vite-plugin-md": "^0.21.5",
    "vitest": "^0.34.3",
    "vue-router-mock": "^1.0.0",
    "vue-tsc": "^1.8.12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/posva/mastering-state-management--code.git"
